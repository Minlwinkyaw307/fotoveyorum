{% extends('home/home/base.html.twig') %}
{% block keywords %} {{setting.keywords}} {% endblock %}
{% block description %} {{setting.description}} {% endblock %}
{% block body %}
    <div class="col-md-9 main">
    <!-- register -->
    <div class="sign-up-form">
        <h3 class="tittle">Registration <i class="glyphicon glyphicon-file"></i></h3>
        <p>Connect other people, start you new journey</p>
        <div class="sign-up">
            <h3 class="tittle reg">Personal Information <i class="glyphicon glyphicon-user"></i></h3>
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <button class="btn btn-primary">{{ button_label|default('Save') }}</button>
            {{ form_end(form) }}
        </div>
    </div>
    <div class="clearfix"> </div>


    <!-- //register -->
{% endblock %}